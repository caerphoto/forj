<div id="dlgUserEditor" title="Edit User">
    <div id="UE_loading" class="loading_msg bb">Loading user details...</div>
    <p>User's name: <span id="UE_name" class="user_detail"></span></p>
    <p>User's e-mail: <span id="UE_email" class="user_detail"></span></p>
    <form>
        <fieldset>
            <legend>User type:</legend>
            <input id="rdoUserType_A" type="radio" name="user_type" value="A" />
            <label for="rdoUserType_A">Admin</label><br />

            <input id="rdoUserType_M" type="radio" name="user_type" value="M" />
            <label for="rdoUserType_M">Moderator</label><br />

            <input id="rdoUserType_N" type="radio" name="user_type" value="N" />
            <label for="rdoUserType_N">Normal</label><br />
        </fieldset>
    </form>
</div>
